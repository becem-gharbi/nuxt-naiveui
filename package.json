{
  "name": "@bg-dev/nuxt-naiveui",
  "version": "1.10.2",
  "description": "Naive UI module for Nuxt 3",
  "author": "Becem Gharbi",
  "keywords": [
    "nuxt",
    "naive-ui",
    "module"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run typecheck && npm run lint && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@iconify/vue": "^4.1.1",
    "@nuxt/kit": "^3.10.0",
    "defu": "^6.1.4",
    "js-cookie": "^3.0.5",
    "naive-ui": "^2.37.3"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.10.0",
    "@nuxt/test-utils": "^3.11.0",
    "@types/js-cookie": "^3.0.6",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "nuxt": "^3.10.0",
    "vitest": "^1.2.2"
  },
  "peerDependencies": {
    "naive-ui": "^2.37.3"
  },
  "repository": {
    "url": "git+https://github.com/becem-gharbi/nuxt-naiveui.git"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
