{
  "name": "@bg-dev/nuxt-naiveui",
  "version": "1.11.0",
  "description": "Naive UI module for Nuxt 3",
  "homepage": "https://nuxt-naiveui.bg.tn",
  "author": "Becem Gharbi",
  "keywords": [
    "nuxt",
    "naive-ui",
    "module"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./utils": {
      "types": "./dist/runtime/utils/index.d.ts",
      "import": "./dist/runtime/utils/index.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run typecheck && npm run lint && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@ant-design/colors": "^7.0.2",
    "@iconify/vue": "^4.1.1",
    "@nuxt/kit": "^3.11.1",
    "defu": "^6.1.4",
    "js-cookie": "^3.0.5",
    "naive-ui": "^2.38.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.11.1",
    "@nuxt/test-utils": "^3.12.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/js-cookie": "^3.0.6",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "nuxt": "^3.11.1",
    "vitest": "^1.4.0"
  },
  "repository": {
    "url": "git+https://github.com/becem-gharbi/nuxt-naiveui.git"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
